#include

using newspace std;

int main()
{
Double Lk, Bk, Hk, Bo, Ho; // Объявление переменных

cout << "Введите длину кирпича: ";
cin >> Lk; // присваиваем введенное значение длины переменной

cout << "Введите ширину кирпича: ";
cin >> Bk; // присваиваем введенное значение ширины переменной

cout << "Введите высоту кирпича: ";
cin >> Hk; // присваиваем введенное значение высоты переменной

cout << "Введите ширину отверстия: ";
cin >> Bo; // присваиваем введенное значение ширины переменной

cout << "Введите высоту отверстия: ";
cin >> Ho; // присваиваем введенное значение высоты переменной

if (Hk <= Ho;Bk<=Bo) {
        cout << "Кирпич входит в отверстие торцом." << endl;
}
if (Hk <= Ho;Lk<=Bo) {
        cout << "Кирпич входит в отверстие боком." << endl;
}
if (Bk <= Ho;Lk<=Bo) {
        cout << "Кирпич входит в отверстие плашмя." << endl;
}
return 0;
}
